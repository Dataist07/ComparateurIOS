        "android": {
          "adaptiveIcon": {
            "foregroundImage": "./assets/adaptive-icon.png",
            "backgroundColor": "#ffffff"
          },
          "package": "com.minh0797.Comparator"
        },
        "web": {
          "favicon": "./assets/favicon.png"
        },
        "extra": {
          "eas": {
            "projectId": "1d2e618d-7111-4215-85ee-57993558c526"
          }
        },
        "plugins": [
          [ 
            "expo-barcode-scanner",
            {
              "cameraPermission": "Nous avons besoin de votre caméra pour pouvoir scanner le numéro code barre de votre carte et l'enregistrer"
            }
          ]
        
        ]
      }
}